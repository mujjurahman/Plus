public function __construct(array $configuration, $plugin_id, $plugin_definition, MailManagerInterface $mail_manager, RendererInterface $renderer) {
  parent::__construct($configuration, $plugin_id, $plugin_definition);
  $this->mailManager = $mail_manager;
  $this->renderer = $renderer;
  \Drupal::logger('pfe_med_connect')->notice('CustomEmailHandler constructed.');
}
public static function create(ContainerInterface $container, array $configuration, $plugin_id, $plugin_definition) {
  return new static(
    $configuration,
    $plugin_id,
    $plugin_definition,
    $container->get('plugin.manager.mail'),
    $container->get('renderer')
  );
}
